{
  "name": "deja-brew",
  "version": "1.0.0",
  "description": "Coffee Recipe Book",
  "author": "A Latte Flav",
  "license": "MIT",
  "main": "server.js",
  "engines": {
    "node": "11.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://b3nitajo.github.io/deja-brew/"
  },
  "homepage": "https://git.heroku.com/deja-brew.git",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm start",
    "install": "cd client && npm install",
    "build": "cd client && npm build",
    "heroku-postbuild": "npm build",
    "kill": "kill-port 3001",
    "lint": "eslint --quiet .",
    "lint:fix": "eslint --fix ."
    },
  "nodemonConfig": {
    "events": {
      "restart": "npm start",
      "crash": "npm start"
    }
  },
  "nyc": {
    "exclude": [
      "client"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "concurrently": "^4.1.1",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "kill-port": "^1.5.1",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sequelize-cli": "^5.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "helmet": "^3.18.0",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.5",
    "mysql2": "^1.6.5",
    "react-materialize": "^3.3.3",
    "sequelize": "^5.8.11"
  }
}
